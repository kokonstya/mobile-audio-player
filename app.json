{
	"expo": {
		"name": "React Native Audio Player",
		"slug": "mobile-audio-player",
		"platforms": [
			"ios",
			"android"
		],
		"version": "1.0.1",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true
		}
	}
}

